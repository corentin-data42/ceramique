{% extends 'admin/base.html.twig' %}

{% block title "Edition de l'oxyde " ~ oxyde.nom %}

{% block body %}
<h1>Edition de l'oxyde {{ oxyde.nom }}</h1>
{{ form_start(form) }}
<div class="d-flex">
    {{ form_row(form.nom) }}
     {{ form_row(form.formule) }}
</div>
<div class="d-flex">
    {{ form_row(form.pm) }}
    {{ form_row(form.type) }}
</div>
{{ form_end(form) }}
{% endblock %}